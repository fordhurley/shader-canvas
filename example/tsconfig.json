{
  "compilerOptions": {
    "module": "commonjs",
    "noLib": false,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "target": "es2017",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true
  }
}
